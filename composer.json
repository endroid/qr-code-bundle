{
    "name": "endroid/qr-code-bundle",
    "description": "Endroid QR Code Bundle",
    "keywords": ["endroid", "qr", "code", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/qr-code-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/qr-code": "dev-master",
        "symfony/framework-bundle": "^3.4||^4.1.12||^5.0",
        "symfony/twig-bundle": "^3.4||^4.0||^5.0",
        "symfony/yaml": "^3.4||^4.0||^5.0"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\QrCodeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\QrCodeBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
